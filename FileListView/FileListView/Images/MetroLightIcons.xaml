<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Contains a list of toolbar (related command) icons
       that can be used in light Metro themes
  -->

  <!-- 16 pixel Folder Up command icon -->
  <BitmapImage x:Key="FLV_Image_FolderUp" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.arrow.up_16.png" />

  <!-- 16 pixel back command icon -->
  <BitmapImage x:Key="FLV_Image_Back" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/MyBase.ArrowLeft_16.png" />

  <!-- 16 pixel forward command icon -->
  <BitmapImage x:Key="FLV_Image_Forward" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/MyBase.ArrowRight_16.png" />

  <!-- 16 pixel refresh command icon -->
  <BitmapImage x:Key="FLV_Image_Refresh" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.refresh_16.png" />

  <!-- 16 pixel IsFolders Visible command icon -->
  <BitmapImage x:Key="FLV_Image_IsFoldersVisible" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.folder_16.png" />

  <!-- 16 pixel refresh command icon -->
  <BitmapImage x:Key="FLV_Image_IsIconVisible" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.image.macro_16.png" />

  <!-- 16 pixel refresh command icon -->
  <BitmapImage x:Key="FLV_Image_IsHiddenFileVisible" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.page.hidden_16.png" />

  <BitmapImage x:Key="FLV_Image_IsFiltered" UriSource="pack://application:,,,/FileListView;component/Images/Metro/Light/appbar.magnify.png" />

</ResourceDictionary>